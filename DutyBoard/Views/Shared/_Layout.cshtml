<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - DutyBoard</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/dashboard.css" />
    <link href="~/css/Style.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css" />
    <!-- fontawesome icon -->
    <link rel="stylesheet" href="~/css/assets/fontawesome-all.min.css">
    <!-- animation css -->
    <link rel="stylesheet" href="~/css/assets/animate.min.css">
    <!-- vendor css -->
    <link rel="stylesheet" href="~/css/assets/style.css">
@*
    <link rel="stylesheet" href="~/css/assets/bootstrap-material-datetimepicker.css">
    <!-- Bootstrap datetimepicker css -->
    <link rel="stylesheet" href="~/css/assets/bootstrap-datepicker3.min.css">
    <link rel="stylesheet" href="~/css/assets/materialdesignicons.min.css">*@
</head>
<body>

    <nav class="pcoded-navbar">
        <div class="navbar-wrapper">
            <div class="navbar-brand header-logo">
                <a asp-controller="Home" asp-action="Index" class="b-brand">
                    @*  <div class="b-bg">
                    <i class="feather icon-trending-up"></i>
                    </div>*@
                    <span class="b-title">Дежурства</span>
                </a>
                <a class="mobile-menu" id="mobile-collapse" href="#!"><span></span></a>
            </div>
            <div class="navbar-content scroll-div">
                <ul class="nav pcoded-inner-navbar">
                    <li class="nav-item" id="Home">
                        <a asp-controller="Home" asp-action="Index" class="nav-link">
                            <span class="pcoded-micon">
                                <i class="fa-solid fa-house feather"></i>
                            </span>
                            <span class="pcoded-mtext">
                                Главная
                            </span>
                        </a>
                    </li>
                    <li class="nav-item" id="Employee">
                        <a asp-controller="Employee" asp-action="Index" class="nav-link">
                            <span class="pcoded-micon">
                                <i class="fa-solid fa-users me-2 feather" width="16"></i>
                            </span>
                            <span class="pcoded-mtext">
                                Сотрудники
                            </span>
                        </a>
                    </li>
                    <li class="nav-item" id="Workday">
                        <a asp-area="" asp-controller="Workday" asp-action="Index" class="nav-link">
                            <span class="pcoded-micon">
                                <i class="fa-solid fa-house-laptop me-2 feather"></i>
                            </span>
                            <span class="pcoded-mtext">
                                Дежурства
                            </span>
                        </a>
                    </li>
                    <li class="nav-item" id="Holiday">
                        <a asp-area="" asp-controller="Holiday" asp-action="Index" class="nav-link">
                            <span class="pcoded-micon">
                                <i class="fa-solid fa-umbrella-beach me-2 feather"></i>
                            </span>
                            <span class="pcoded-mtext">
                                Отпуска
                            </span>
                        </a>
                    </li>
                    <li class="nav-item" id="Roster">
                        <a asp-area="" asp-controller="Roster" asp-action="Index" class="nav-link">
                            <span class="pcoded-micon">
                                <i class="fa-solid fa-screwdriver-wrench me-2 feather"></i>
                            </span>
                            <span class="pcoded-mtext">
                                Параметры
                            </span>
                        </a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>

    <section class="pcoded-main-container">
        <div class="pcoded-wrapper">
            <div class="pcoded-content">
                <div class="main-body">
                    <div class="page-wrapper">
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="card">
                                    <div class="card-header">
                                        <h5>Параметры</h5>

                                    </div>
                                    <div class="card-block text-center">
                                        <div class="row">
                                            <partial name="_Notifications" />
                                            @RenderBody()
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div>
                                <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
                                    <div class="chartjs-size-monitor"><div class="chartjs-size-monitor-expand"><div class=""></div></div><div class="chartjs-size-monitor-shrink"><div class=""></div></div></div>

                                </main>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>
    <script src="https://kit.fontawesome.com/beb727a5bd.js" crossorigin="anonymous"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/dashboard.js" asp-append-version="true"></script>
    <script src="~/js/assets/vendor-all.min.js"></script>
    <script src="~/js/assets/pcoded.min.js"></script>
    <script src="~/js/Layout.js"></script>
@*    <script src="~js/assets/bootstrap-material-datetimepicker.js"></script>
    <script src="~js/assets/form-picker-custom.js"></script>
    <script src="http://momentjs.com/downloads/moment-with-locales.min.js"></script>*@
    @RenderSection("Scripts", required: false)
</body>
</html>
